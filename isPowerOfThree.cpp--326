class Solution {
public:
    bool isPowerOfThree(int n) {
        while(n>3){
            if(n%3 != 0){
                return false;
            }
            n/=3;
        }
        return true;
    }
};
int main(){
    Solution* solution = new Solution;
    std::cout<<solution->isPowerOfThree(9)<<std::endl;

    return 0;
}
